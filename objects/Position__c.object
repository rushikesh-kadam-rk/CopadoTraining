        <fullName>Close_Date_Rule</fullName>
        <active>false</active>
        <description>Close Date must be specified when Status is set to &apos;Closed - Filled&apos; or &apos;Closed - Not Approved.&apos;</description>
        <errorConditionFormula>ISBLANK(  Date_Closed__c ) &amp;&amp; ISPICKVAL( Status__c , &quot;Closed&quot;) &amp;&amp; ( ISPICKVAL(  Sub_Status__c , &quot;Filled&quot;)|| ISPICKVAL(  Sub_Status__c , &quot;Not Approved&quot;))|| Date_Closed__c &lt;  NOW()</errorConditionFormula>
        <errorDisplayField>Date_Closed__c</errorDisplayField>
        <errorMessage>Closed date must be specified when status is set to &apos;Closed&apos;.</errorMessage>
    </validationRules>
