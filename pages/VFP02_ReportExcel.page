<apex:page controller="VFC02_VFP02_ReportExcel"  contentType="application/vnd.ms-excel#ProfileCompare.xls" readOnly="true" sidebar="false">
<font color="RED"><apex:pageMessages /></font>

<!-- Custom App Settings -->
<apex:outputPanel rendered="{!if(lstCA.size==0,false,true)}">
<br/>
<b>Custom App Settings</b>   
<table border="1">
    <tr bgcolor="#dddddd">
        <th>Profiles</th>    
        <apex:repeat value="{!lstCA}" var="caNames">
            <th colspan="2" align="center">{!caNames}</th>
        </apex:repeat>
    </tr>
    <tr bgcolor="#dddddd">
        <td/>
        <apex:repeat value="{!lstCA}" var="caNames">
            <th>Visible</th>
            <th>Default</th>
        </apex:repeat>
    </tr>
        
    <apex:repeat value="{!mapCA}" var="recs">
        <tr>
            <td>{!recs}</td>
            <apex:repeat value="{!mapCA[recs]}" var="rVal">
                <td><Font color="{!Left(mapCAColors[recs][rVal],FIND('-',mapCAColors[recs][rVal])-1)}">{!Left(mapCA[recs][rVal],FIND('-',mapCA[recs][rVal])-1)}</Font></td>
                <td><Font color="{!RIGHT(mapCAColors[recs][rVal],LEN(mapCAColors[recs][rVal]) - FIND('-',mapCAColors[recs][rVal]))}">{!RIGHT(mapCA[recs][rVal],LEN(mapCA[recs][rVal]) - FIND('-',mapCA[recs][rVal]))}</Font></td>
            </apex:repeat>
        </tr>
    </apex:repeat> 
</table>
</apex:outputPanel>

